{
    "manifest_version": 1,
    "name": "unlimited AppBrain",
    "version": "0.1.0",
    "description": "Chrome extension to get unlimited access in appbrain",
    "content_scripts": [
        {
            "matches": [
                "https://www.appbrain.com/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "index.html"
    },
    "icons": {
        "16": "icon_16.png",
        "48": "icon_48.png",
        "128": "icon_128.png"
    }
}